{
    "name": "recipe-runner/recipe-runner",
    "description": "",
    "type": "library",
    "keywords": [
        "yaml"
    ],
    "homepage": "https://github.com/recipe-runner/recipe-runner",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com",
            "homepage": "http://yosymfony.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "yosymfony/collection": "^1.1",
        "symfony/expression-language": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "autoload": {
        "psr-4": { "RecipeRunner\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "RecipeRunner\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpstan/phpstan": "^0.10.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "stan": "vendor/bin/phpstan analyse src"
    }
}
